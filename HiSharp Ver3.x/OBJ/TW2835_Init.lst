  77   1        // Waitting Color Lock
  78   1        EnUserTimer(USERTMID1,10);
  79   1        while(USERTMVAL1!=1) {
  80   2          if( (TW28_ReadByte(0, SENSORCHANNEL<<4)&0x0F) == 0x0F) {
  81   3            SYSTYPE = PCT_DetectVideoFormat(SENSORCHANNEL);
  82   3            break;
  83   3          }
  84   2          Wait_ms(10);
  85   2        }
  86   1        DisUserTimer(USERTMID1);
  87   1        
  88   1        PCT_TW2835_Initial();
